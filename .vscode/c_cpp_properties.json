{
    "env": {
        "myIncludePath": [
            "${workspaceFolder}/include",
            "${workspaceFolder}/src",
            "${workspaceFolder}/lib",
            "${workspaceFolder}/deps",
            "${workspaceFolder}/po",
            "${workspaceFolder}/etc",
            "${workspaceFolder}/docs",
            "${workspaceFolder}/dist",
            "${workspaceFolder}/logs",
            "${workspaceFolder}/tmp",
            "${workspaceFolder}/build",
            "${workspaceFolder}/objects",
            "/usr/include",
            "/usr/local/include"
        ],
        "myDefines": [
            "DEBUG",
            "MY_FEATURE=1",
            "_POSIX_C_SOURCE=GNU23",
            "_XOPEN_SOURCE=800",
            "PLATFORM_LINUX"
        ]
    },
    "configurations": [
        {
            "name": "Linux",
            "compilerPath": "/usr/bin/gcc",
            "compilerArgs": [
                "-std=gnu23",
                "-O0",
                "-fstack-protector",
                "-march=native",
                "-mtune=generic",
                "-fPIC",
                "-fno-common",
                "-ffunction-sections",
                "-fdata-sections",
                "-flto",
                "-funroll-all-loops",
                "-Wl,--gc-sections",
                "-Wall",
                "-Wextra",
                "-pedantic",
                "-Wconversion",
                "-Wfloat-equal",
                "-fvisibility=hidden",
                "-fno-strict-aliasing",
                "-fdiagnostics-color=always",
                "-fuse-ld=mold",
                "-I/usr/local/include/glib-2.0",
                "-I/usr/local/lib/glib-2.0/include",
                "-I/opt/homebrew/include/glib-2.0",
                "-I/opt/homebrew/lib/glib-2.0/include"
            ],
            "intelliSenseMode": "linux-gcc-x64",
            "includePath": [
                "${myIncludePath}"
            ],
            "defines": [
                "${myDefines}"
            ],
            "cStandard": "gnu23",
            "cppStandard": "gnu++23",
            "forcedInclude": [
                "${workspaceFolder}/include/config.h"
            ],
            "browse": {
                "path": [
                    "${myIncludePath}",
                    "${workspaceFolder}"
                ],
                "limitSymbolsToIncludedHeaders": true,
                "databaseFilename": "${workspaceFolder}/.vscode/browse.vc.db"
            }
        },
        {
            "name": "Mac",
            "compilerPath": "/opt/homebrew/Cellar/gcc/14.2.0_1/bin/gcc-14",
            "compilerArgs": [
                "-std=gnu23",
                "-O0",
                "-fstack-protector",
                "-march=native",
                "-mtune=generic",
                "-fPIC",
                "-fno-common",
                "-ffunction-sections",
                "-fdata-sections",
                "-flto",
                "-funroll-all-loops",
                "-Wl,--gc-sections",
                "-Wall",
                "-Wextra",
                "-pedantic",
                "-Wconversion",
                "-Wfloat-equal",
                "-fvisibility=hidden",
                "-fno-strict-aliasing",
                "-fdiagnostics-color=always",
                "-fuse-ld=mold",
                "-I/usr/local/include/glib-2.0",
                "-I/usr/local/lib/glib-2.0/include",
                "-I/opt/homebrew/include/glib-2.0",
                "-I/opt/homebrew/lib/glib-2.0/include"
            ],
            "intelliSenseMode": "macos-gcc-arm64",
            "includePath": [
                "${workspaceFolder}/include",
                "${workspaceFolder}/src",
                "${workspaceFolder}/lib",
                "${workspaceFolder}/deps",
                "${workspaceFolder}/po",
                "${workspaceFolder}/etc",
                "${workspaceFolder}/docs",
                "${workspaceFolder}/dist",
                "${workspaceFolder}/logs",
                "${workspaceFolder}/tmp",
                "${workspaceFolder}/build",
                "${workspaceFolder}/objects",
                "/opt/homebrew/Cellar/gcc/14.2.0_1/lib/gcc/current/gcc/aarch64-apple-darwin24/14/include",
                "/opt/homebrew/Cellar/gcc/14.2.0_1/lib/gcc/current/gcc/aarch64-apple-darwin24/14/include-fixed",
                "/Library/Developer/CommandLineTools/SDKs/MacOSX.sdk/usr/include",
                "/Library/Developer/CommandLineTools/usr/lib/clang/16.0.0/include"
            ],
            "defines": [
                "DEBUG",
                "MY_FEATURE=1",
                "_POSIX_C_SOURCE=2024L",
                "_XOPEN_SOURCE=800",
                "PLATFORM_MAC"
            ],
            "cStandard": "gnu23",
            "cppStandard": "gnu++23",
            "forcedInclude": [
                "${workspaceFolder}/include/config.h"
            ],
            "browse": {
                "path": [
                    "${workspaceFolder}/include",
                    "${workspaceFolder}/src",
                    "${workspaceFolder}/lib",
                    "${workspaceFolder}/deps",
                    "${workspaceFolder}/po",
                    "${workspaceFolder}/etc",
                    "${workspaceFolder}/docs",
                    "${workspaceFolder}/dist",
                    "${workspaceFolder}/logs",
                    "${workspaceFolder}/tmp",
                    "${workspaceFolder}/build",
                    "${workspaceFolder}/objects",
                    "/opt/homebrew/Cellar/gcc/14.2.0_1/lib/gcc/current/gcc/aarch64-apple-darwin24/14/include",
                    "/opt/homebrew/Cellar/gcc/14.2.0_1/lib/gcc/current/gcc/aarch64-apple-darwin24/14/include-fixed",
                    "/Library/Developer/CommandLineTools/SDKs/MacOSX.sdk/usr/include",
                    "/Library/Developer/CommandLineTools/usr/lib/clang/16.0.0/include"
                ],
                "limitSymbolsToIncludedHeaders": true,
                "databaseFilename": "${workspaceFolder}/.vscode/browse.vc.db"
            }
        }
    ],
    "version": 4,
    "enableConfigurationSquiggles": true
}
